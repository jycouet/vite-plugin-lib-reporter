{
  "name": "vite-plugin-lib-reporter",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jycouet/vite-plugin-lib-reporter"
  },
  "type": "module",
  "bin": {
    "libReporter": "cli.js"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "pck": "pnpm build && svelte-package && node ./tools/tweakPck",
    "preview": "vite preview",
    "format": "prettier --write --list-different .",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "release": "pnpm pck && changeset publish"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.7",
    "@changesets/cli": "^2.25.2",
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "1.0.0-next.6",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@theguild/prettier-config": "^1.0.0",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/d3-format": "^3.0.1",
    "@types/d3-hierarchy": "^3.1.0",
    "d3-format": "^3.1.0",
    "d3-hierarchy": "^3.1.2",
    "layercake": "^7.1.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "5.4.4"
  },
  "dependencies": {
    "@kitql/helpers": "^0.8.10",
    "terser": "^5.15.1"
  },
  "publishConfig": {
    "directory": "package",
    "access": "public"
  }
}